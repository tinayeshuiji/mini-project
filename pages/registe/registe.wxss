page{
  background: #fff;
}
.registe-box{
  width:90%;
  margin: 0 auto;
  padding:70rpx 0;
}
.icon01{
  width:161rpx;
  height: 161rpx;
  display: block;
}
.register-line{
  width:100%;
  height: 120rpx;
  border-bottom:1px solid #d7d7d7;

}
.register-line text{
  width:150rpx;
  height: 100%;
  line-height: 120rpx;
  display: block;
}
.phone-input{
  width:calc(100% - 100rpx);
  height: 100%;
  line-height: 120rpx;
}
.code-input{
  width:calc(100% - 350rpx);
  height: 100%;
  line-height: 120rpx;
}
.code{
  width:200rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border-radius: 70rpx;
  color: #fff;
  background-color: #9cc829;
}
.code-time {
  width:200rpx;
   height:80rpx;
  line-height: 80rpx;
  background:#ddd;
  text-align: center;
  border-radius: 80rpx;
  color: #fff;
}
.money{
  width:100%;
  color: #010101;
  text-align: left;
  height: 100rpx;
  line-height: 100rpx;

}
.money text{
  color: #fc2e07;
  font-size: 30rpx;
}
.register-btn{
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  border-radius: 100rpx;
  font-size: 35rpx;
  background-color: #9cc829;
  margin-top: 50rpx;
}
.icon-check {
  width: 48rpx;
  height: 48rpx;
  margin-right: 10rpx;
}
.icon-check image {
  width: 100%;
  height: 100%;
  display: block;
}